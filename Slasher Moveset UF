local currentlyPlayingTrack = nil
local currentlyPlayingSound = nil
local blockedAudioIds = {621557962, 0}
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

local function destroyBlockedSounds()
    for _, child in ipairs(rootPart:GetChildren()) do
        if child:IsA("Sound") then
            local id = tonumber(string.match(child.SoundId, "%d+"))
            for _, blockedId in ipairs(blockedAudioIds) do
                if id == blockedId then
                    child:Stop()
                    child:Destroy()
                end
            end
        end
    end
end

spawn(function()
    while true do
        destroyBlockedSounds()
        wait(0.05)
    end
end)

local function handleAnimationDetection(animIdsToStop, replacementAnimId, replacementAnimSpeed, replacementAudioId, replacementAudioVolume)
    local function onAnimPlayed(animationTrack)
        local animId = tonumber(string.match(animationTrack.Animation.AnimationId, "%d+"))
        for _, stopId in ipairs(animIdsToStop) do
            if animId == stopId then
                if currentlyPlayingTrack then
                    currentlyPlayingTrack:Stop(0)
                    currentlyPlayingTrack = nil
                end
                animationTrack:Stop(0)
                if currentlyPlayingSound then
                    currentlyPlayingSound:Stop()
                    currentlyPlayingSound:Destroy()
                    currentlyPlayingSound = nil
                end
                local replacementAnimation = Instance.new("Animation")
                replacementAnimation.AnimationId = "rbxassetid://" .. replacementAnimId
                local replacementTrack = humanoid:LoadAnimation(replacementAnimation)
                replacementTrack:Play(0)
                replacementTrack:AdjustSpeed(replacementAnimSpeed or 1)
                currentlyPlayingTrack = replacementTrack
                if replacementAudioId then
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://" .. replacementAudioId
                    sound.Looped = false
                    sound.Volume = replacementAudioVolume or 1
                    sound.Parent = rootPart
                    sound:Play()
                    currentlyPlayingSound = sound
                end
                break
            end
        end
    end
    humanoid.AnimationPlayed:Connect(onAnimPlayed)
end

local firstAnimIdsToStop = {10469493270}
local firstReplacementAnimId = 89044067797964
local firstReplacementAnimSpeed = 1
local firstReplacementAudioId = 7122613461
local firstReplacementAudioVolume = 1
handleAnimationDetection(firstAnimIdsToStop, firstReplacementAnimId, firstReplacementAnimSpeed, firstReplacementAudioId, firstReplacementAudioVolume)

local secondAnimIdsToStop = {10469630950}
local secondReplacementAnimId = 74334194837918
local secondReplacementAnimSpeed = 1
local secondReplacementAudioId = 7122613461
local secondReplacementAudioVolume = 0.8
handleAnimationDetection(secondAnimIdsToStop, secondReplacementAnimId, secondReplacementAnimSpeed, secondReplacementAudioId, secondReplacementAudioVolume)

local thirdAnimIdsToStop = {10469639222}
local thirdReplacementAnimId = 94353845974131
local thirdReplacementAnimSpeed = 1
local thirdReplacementAudioId = 7122613461
local thirdReplacementAudioVolume = 0.7
handleAnimationDetection(thirdAnimIdsToStop, thirdReplacementAnimId, thirdReplacementAnimSpeed, thirdReplacementAudioId, thirdReplacementAudioVolume)

local fourthAnimIdsToStop = {10469643643}
local fourthReplacementAnimId = 80601239139774
local fourthReplacementAnimSpeed = 1
local fourthReplacementAudioId = 7122613461
local fourthReplacementAudioVolume = 1
handleAnimationDetection(fourthAnimIdsToStop, fourthReplacementAnimId, fourthReplacementAnimSpeed, fourthReplacementAudioId, fourthReplacementAudioVolume)

