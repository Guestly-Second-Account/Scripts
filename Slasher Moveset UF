function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Volume = 1
    sound.Looped = true
    sound.PlaybackSpeed = 1
    sound.Name = "SucK"
   sound:Play()
end

CustomGitSound("https://github.com/Guestly-Second-Account/Scripts/raw/refs/heads/main/iLoveYt.net_YouTube_Guest-666-VS-Noob-OST-Placeholder-Forsak_Media_vdMTs58Cdsk_004_360p.mp3?raw=true", 1, " SucK1")

if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
game.Workspace.SucK:Destroy()
end

